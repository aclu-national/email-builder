<!-- START OF SOCIAL BOXES -->
<table role="presentation" align="center" border="0" cellpadding="0" cellspacing="0" class="email-wrapper" width="600">
	<tbody>
		<tr>
			<td class="darkmode-box" style="font-family:  Helvetica, Arial, sans-serif; font-size:16px; color:#2a2a2a; padding:0px 40px 0px 40px;border-radius: 0 0 3px 3px;" bgcolor="#f2f2f2">
				<table role="presentation" bgcolor="#f2f2f2" width="100%" border="0" cellspacing="0" cellpadding="0" style="background-color:#f2f2f2;border-radius: 3px 3px 0 0;">
					<tr>
						<td bgcolor="#ef404d" style="padding:20px 0px 0px 0px;border-radius: 3px 3px 0 0;">&nbsp; </td>
					</tr>
				</table>
			</td>
		</tr>

		<!-- MAIN FLOATING BOX -->
		<tr>
			<td	style="font-family:  Helvetica, Arial, sans-serif; font-size:16px; color:#2a2a2a; padding:0px 40px 0px 40px;" bgcolor="transparent">
				<table role="presentation" bgcolor="#ef404d" width="100%" border="0" cellspacing="0" cellpadding="0" style="background-color:#ef404d;">
					<tbody>
						<tr>
							<td valign="top" style="padding:0px 0px 0px 0px;font-family: Arial,'Helvetica Neue',sans-serif;font-size:16px;line-height:20px;color:#ffffff;text-align:center;background-color:#ef404d;">
								<strong>Follow us on social:</strong>
							</td>
						</tr>
					</tbody>
				</table>
			</td>
		</tr>
		<!-- MAIN FLOATING BOX -->

		<tr>
			<td	class="darkmode-box" style="font-family:  Helvetica, Arial, sans-serif; font-size:16px; color:#2a2a2a; padding:0px 40px 0px 40px;background-color:#f2f2f2;border-radius: 3px 3px 0 0;">
				<table role="presentation" bgcolor="#ef404d" width="100%" border="0" cellspacing="0" cellpadding="0" style="background-color:#ef404d;border-radius: 0 0 3px 3px;">
					<tr>
						<td bgcolor="#ef404d" style="padding:10px 0px 20px 0px;text-align: center;border-radius: 0 0 3px 3px;">
							<a href="{facebook_url}"><img src="{facebook_logo}" border="0" alt="Facebook" width="33" /></a> &nbsp; &nbsp; <a href="{x_url}"><img src="{x_logo}" border="0" alt="X" width="33" /></a> &nbsp; &nbsp; <a href="{instagram_url}"><img src="{instagram_logo}" border="0" alt="Instagram" width="33" /></a>
						</td>
					</tr>
				</table>
			</td>
		</tr>
	</tbody>
</table>
<!-- END OF SOCIAL BOXES -->